<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
          <service name="Library.WcfService.SoaTestService">
            <endpoint address="net.tcp://localhost:8734/Design_Time_Addresses/Library.WcfService/SoaTestService/ "
                binding="netTcpBinding" contract="Library.WcfService.ISoaTestService">
              <identity>
                <dns value="localhost" />
              </identity>
            </endpoint>
            <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
            <host>
              <baseAddresses>
                <add baseAddress="http://localhost:8733/Design_Time_Addresses/Library.WcfService/SoaTestService/" />
              </baseAddresses>
            </host>
          </service>
             
        </services>
    </system.serviceModel>
</configuration>